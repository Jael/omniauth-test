<h1>Where can we find you?</h1>
<%= render layout: "form" do |f| %>
   <div class="field">
      <%=f.label :website  %><br />
      <%= f.text_field :website %>
   </div>
   <div class="field">
      <%= f.label :qq, "QQ"  %><br />
      <%= f.text_field :qq %>
    </div>
    <div class="field">
      <%= f.label :telephone %><br />
      <%= f.text_field :telephone %>
    </div>
<% end %>
